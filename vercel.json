{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "login.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/login.html", "dest": "/login.html" },
    { "src": "/login", "dest": "/login.html" },
    { "src": "/index.html", "dest": "/index.html" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
